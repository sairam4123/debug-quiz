{
    "headers": [
        {
            "source": "/api/trpc/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                },
                {
                    "key": "X-Accel-Buffering",
                    "value": "no"
                }
            ]
        }
    ]
}